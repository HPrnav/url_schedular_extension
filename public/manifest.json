{
    "manifest_version": 3,
    "name": "SCHEDULAR",
    "version": "1.0",
    "permissions": [
      "alarms",
      "storage",
      "tabs",
      "activeTab",
      "scripting"
    ],
    "icons":{
        "16":"url.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  